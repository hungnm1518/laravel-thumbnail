{
  "name": "hungnm1518/laravel-thumbnail",
  "description": "Laravel Package to resize images on the fly by Hung Nguyen <hungnm144@gmail.com>",
  "keywords": [
    "Laravel",
    "Image",
    "Resize",
    "Thumbnail"
  ],
  "homepage": "https://fb.com/hungnm1518/",
  "license": "MIT",
  "authors": [
    {
      "name": "Hung Nguyen",
      "email": "hungnm144@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
    "intervention/image": "^2.5|^3.0"
  },  
  "autoload": {
    "psr-4": {
      "HungNM\\LaravelThumbnail\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "HungNM\\LaravelThumbnail\\LaravelThumbnailServiceProvider"
      ],
      "aliases": {
        "Thumbnail": "HungNM\\LaravelThumbnail\\Facades\\LaravelThumbnail"
      }
    }
  }
}
